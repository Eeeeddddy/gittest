<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Sample Category"
            id="randoop">
      </category>
      <view
            name="Randoop"
            icon="icons/sample.gif"
            category="randoop"
            class="randoop.plugin.views.RandoopView"
            id="randoop.plugin.views.RandoopView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.TaskList"
               relationship="right"
               id="randoop.plugin.views.RandoopView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="randoop.preferences.SamplePreferencePage"
            id="randoop.preferences.SamplePreferencePage"
            name="Sample Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            adaptable="true"
            class="randoop.plugin.properties.ProjectPropertyPage"
            id="randoop.plugin.projectPage"
            name="Randoop"
            objectClass="org.eclipse.core.resources.IProject">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="randoop.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
          <launchConfigurationType
            name="Randoop Launcher"
            delegate="randoop.plugin.launching.RandoopLaunchConfigDelegate"
            modes="run, debug"
            id="randoop.plugin.launching.launchconfig">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
         
          <launchConfigurationTabGroup
            type="randoop.plugin.launching.launchconfig"
            class="randoop.plugin.launching.RandoopLaunchConfigTabGroup"
            id="randoop.plugin.launching.launchconfig.tabgroup">
      </launchConfigurationTabGroup> 
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="randoop.plugin.launching.launchconfig"
            icon="icons/sample.gif"
            id="randoop.plugin.launching.launchconfig.image">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.ltk.core.refactoring.renameParticipants">
      <renameParticipant
            class="randoop.plugin.internal.core.refactoring.ITypeRenameParticipant"
            id="randoop.plugin.refactoring.launching.typeRename"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IType">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="randoop.plugin.internal.core.refactoring.IPackageFragmentRenameParticipant"
            id="randoop.plugin.refactoring.launching.packageRename"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IPackageFragment">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="randoop.plugin.internal.core.refactoring.IJavaProjectRenameParticipant"
            id="randoop.plugin.refactoring.launching.projectRename"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IJavaProject">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="randoop.plugin.internal.core.refactoring.ITypeRenameParticipant"
            id="randoop.plugin.refactoring.launching.methodRename"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IMethod">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
      <renameParticipant
            class="randoop.plugin.internal.core.refactoring.IPackageFragmentRootRenameParticipant"
            id="randoop.plugin.refactoring.launching.folderRename"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IPackageFragmentRoot">
               </instanceof>
            </with>
         </enablement>
      </renameParticipant>
   </extension>
   <extension
         point="org.eclipse.ltk.core.refactoring.moveParticipants">
      <moveParticipant
            class="randoop.plugin.internal.core.refactoring.ITypeMoveParticipant"
            id="randoop.plugin.refactoring.launching.typeMove"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IType">
               </instanceof>
            </with></enablement>
      </moveParticipant>
      <moveParticipant
            class="randoop.plugin.internal.core.refactoring.IPackageFragmentMoveParticipant"
            id="randoop.plugin.refactoring.launching.packageMove"
            name="Launch configuration participant">
         <enablement>
            <with
                  variable="affectedNatures">
               <iterate
                     operator="or">
                  <equals
                        value="org.eclipse.jdt.core.javanature">
                  </equals>
               </iterate>
            </with>
            <with
                  variable="element">
               <instanceof
                     value="org.eclipse.jdt.core.IPackageFragment">
               </instanceof>
            </with></enablement>
      </moveParticipant>
   </extension>
  </plugin>
